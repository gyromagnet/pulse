{
  "exclude_tree": [".venv/", ".git/", "__pycache__", ".pytest_cache/", "node_modules/*"],
  "exclude_print": [
    "public/example_code/*",
    "public/grammars/*",
    "docs/",
    "*.egg-info",
    "*.ipynb",
    ".gitignore",
    ".gitmodules",
    ".groblignore",
    ".python-version",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "LICENSE",
    ".coverage.xml",
    "ruff.default.toml",
    "uv.lock",
    "yarn.lock",
    "package-lock.json"
  ],
  "include_tree_tags": "tree",
  "include_file_tags": "file"
}
